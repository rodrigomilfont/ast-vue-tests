{
  "name": "ast-vue",
  "version": "1.0.4",
  "description": "",
  "keywords": [
    "typescript",
    "template"
  ],
  "author": {
    "name": "DW",
    "url": "http://local"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/rodrigomilfont/ast-vue-tests.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && tsc",
    "changelog": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "clean": "(rm -r ./.nyc_output || true) && (rm -r ./coverage || true) && (rm -r ./dist || true)",
    "clean:all": "npm run clean && (rm -r ./node_modules || true)",
    "cover": "nyc npm run test",
    "cover:check": "nyc --check-coverage npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --ext .ts --fix src/**/*.ts src/**/*.spec.ts",
    "preversion": "npm run build && npm run cover:check",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "test": "mocha -r ts-node/register -r source-map-support/register src/**/*.spec.ts",
    "upgrade": "npx npm-check -u",
    "version": "npm run build && npm run changelog:update",
    "dev": "nodemon --watch 'src/**' --ext 'ts' --exec 'ts-node --project tsconfig.json src/index.ts'"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "chai": "^4.1.2",
    "conventional-changelog-cli": "^2.2.2",
    "coveralls": "^3.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^8.34.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.18",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^10.5.0",
    "typescript": "^4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/glob": "^7.2.0",
    "eslint-plugin-vue": "^8.4.1",
    "eslint-visitor-keys": "^3.3.0",
    "glob": "^7.2.0",
    "sitka": "^1.0.5",
    "vue-eslint-parser": "^8.2.0",
    "vue-template-compiler": "^2.6.14"
  }
}
